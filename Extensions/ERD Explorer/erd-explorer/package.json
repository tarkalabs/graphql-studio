{
  "activationEvents": [
    "onCommand:erd-explorer.generateERD",
    "onCommand:erd-explorer.viewERD"
  ],
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "command": "erd-explorer.generateERD",
        "title": "Generate ERD"
      },
      {
        "command": "erd-explorer.viewERD",
        "title": "View ERD"
      }
    ]
  },
  "dependencies": {
    "bootstrap": "^4.5.0",
    "db-utils": "file:../../../Npm/db-utils",
    "jquery": "^3.5.1",
    "mermaid": "^8.5.2",
    "popper.js": "^1.16.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-mermaid2": "^0.1.4"
  },
  "description": "",
  "devDependencies": {
    "@types/bootstrap": "^4.5.0",
    "@types/glob": "^7.1.1",
    "@types/jquery": "^3.5.0",
    "@types/mermaid": "^8.2.1",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.0.14",
    "@types/popper.js": "^1.11.0",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.3.1",
    "glob": "^7.1.6",
    "mocha": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^7.0.5",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3",
    "vscode": "^1.1.37",
    "vscode-test": "^1.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "displayName": "ERD Explorer",
  "engines": {
    "vscode": "^1.46.0"
  },
  "main": "./out/extension.js",
  "name": "erd-explorer",
  "scripts": {
    "compile": "npm-run-all compile:*",
    "compile:extension": "tsc -p ./",
    "compile:views": "webpack --mode development",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test",
    "vscode:prepublish": "npm run compile",
    "watch": "npm-run-all -p watch:*",
    "watch:extension": "tsc -watch -p ./",
    "watch:views": "webpack --watch --mode development"
  },
  "version": "0.0.1"
}
