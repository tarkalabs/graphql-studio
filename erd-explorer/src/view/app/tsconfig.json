{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "node",
        "target": "es6",
        "outDir": "erdViewer",
        "lib": [
            "es6",
            "dom"
        ],  
        "jsx": "react",
        "sourceMap": true,
        "rootDir": "..",
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "experimentalDecorators": true,
        "typeRoots": [ "../node_modules/@types" ],
        "baseUrl": ""
    },
    "exclude": [
        "node_modules"
    ]
}